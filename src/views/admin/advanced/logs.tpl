<div class="logs">
	<div class="col-lg-9">
		<div class="panel panel-default">
			<div class="panel-heading"><i class="fa fa-file-text-o"></i> 日志</div>
			<div class="panel-body">
				<pre>{data}</pre>
			</div>
		</div>
	</div>
	<div class="col-lg-3 acp-sidebar">
		<div class="panel panel-default">
			<div class="panel-heading">日志控制面板</div>
			<div class="panel-body">
				<button class="btn btn-primary" data-action="reload"><i class="fa fa-refresh"></i> 刷新记录</button>
				<button class="btn btn-warning" data-action="clear"><i class="fa fa-eraser"></i> 清除日志</button>
			</div>
		</div>
	</div>
</div>
